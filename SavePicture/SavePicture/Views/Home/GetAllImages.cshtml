@model IEnumerable<SavePicture.Models.Images>

@{
    ViewBag.Title = "Images";
}
<h2>@ViewBag.Title</h2>


@foreach (var i in Model)
{
    <a href="@Url.Action(  "GetImage", "Home",  new {id = i.Id})">
        <img src="/Home/DisplayingImage?Id=@i.Id" class="img-rounded" style="margin:5px; width: calc(20% - 10px); display:inline;" />
    </a>

}


